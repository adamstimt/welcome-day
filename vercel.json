{
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "framework": "vite",
  "env": {
    "NODE_VERSION": "20.19.0"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
